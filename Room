import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Set;
/**
 * Represents the the different rooms on the board. Holds information about the locations and objects held within the room.
 * @author hildrecall and rehanmidh
 *
 */
public class Room {
  private String name;
  private List<Location> entrys;
  private Set<Weapon> weapons = new HashSet<Weapon>();
  private Set<Person> people = new HashSet<Person>();
  public Room(String name, List<Location> entrys) {
	  this.name = name;
	  this.entrys = entrys;
  }

  //getters

  public String getName() {return this.name;}
  public List<Location> getEntrys(){return entrys;}
  public Set<Weapon> getWeapons(){return weapons;}
  public Set<Person> getPeople(){return people;}
}
