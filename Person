
public class Person {
  //Represents the game characters
  //Contains name and the starting location of the person
  String name;
  Location startinglocation;
  public Person(String name, Location location) {
	  this.name = name;
	  startinglocation = location;
  }

  public String getName() {return this.name;}
  public Location getStart() {return this.startinglocation;}
}
